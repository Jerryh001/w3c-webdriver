{
  "name": "w3c-webdriver",
  "version": "0.14.0",
  "description": "W3C WebDriver client",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json && tsc --noEmit",
    "test": "jest --detectOpenHandles",
    "test:chrome": "cross-env BROWSER=chrome npm test",
    "test:chrome-headless": "cross-env BROWSER=chrome-headless npm test",
    "test:firefox": "cross-env BROWSER=firefox npm test",
    "test:internet-explorer": "cross-env BROWSER=internet-explorer npm test",
    "docs": "documentation ../../readme src/index.js --section=API",
    "build": "del-cli lib && tsc --emitDeclarationOnly -p tsconfig.build.json && babel src --out-dir lib --extensions \".ts\"",
    "prepack": "npm run build && npm run docs",
    "add:contributor": "all-contributors add",
    "generate:contributors": "all-contributors generate"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "browser",
    "automation",
    "testing",
    "test",
    "end-to-end",
    "e2e",
    "integration"
  ],
  "repository": "git@github.com:mucsi96/w3c-webdriver.git",
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chromedriver": "^2.38.0",
    "@types/debug": "^4.1.2",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.9.5",
    "@types/portscanner": "^2.1.0",
    "@types/wait-on": "^3.2.0",
    "all-contributors-cli": "^6.0.0",
    "chromedriver": "^2.43.0",
    "cross-env": "5.2.0",
    "del-cli": "^1.1.0",
    "documentation": "^9.1.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "geckodriver": "^1.12.2",
    "iedriver": "^3.9.2",
    "jest": "^24.0.0",
    "portscanner": "^2.2.0",
    "tslint": "^5.13.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.3.3333",
    "wait-on": "^3.1.0"
  }
}
